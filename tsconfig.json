{
	"compilerOptions": {
		"noEmit": true,
		"module": "esnext",
		"target": "es2022",
		"lib": [
			"es2022"
		],
		"strict": true,
		"alwaysStrict": true,
		"preserveConstEnums": true,
		"moduleResolution": "node",
		"sourceMap": true,
		"types": [
			"@cloudflare/workers-types",
			"@types/jest"
		]
	},
	"include": [
		"src"
	],
	"exclude": [
		"node_modules",
		"dist",
		"test"
	]
}